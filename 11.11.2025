#Bubble sort
def bubble_sort(lista):
    interschimbat = True
    while interschimbat:
        interschimbat = False
        i = 0
        while i < len(lista) - 1:
            if lista[i] > lista[i + 1]:
                lista[i], lista[i + 1] = lista[i + 1], lista[i]
                interschimbat = True
            i += 1
        return lista
#Exemplu de utilizare
numere = [64, 34, 25, 12, 22, 11, 90]
numere_sortate = bubble_sort(numere)
print("Lista sortata este:", numere_sortate)